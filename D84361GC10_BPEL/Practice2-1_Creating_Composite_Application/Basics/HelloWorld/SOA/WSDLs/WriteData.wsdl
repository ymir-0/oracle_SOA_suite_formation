<wsdl:definitions
     name="WriteData"
     targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/Basics/HelloWorld/WriteData"
     xmlns:tns="http://xmlns.oracle.com/pcbpel/adapter/file/Basics/HelloWorld/WriteData"
     xmlns:jca="http://xmlns.oracle.com/pcbpel/wsdl/jca/"
     xmlns:plt="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:imp1="http://www.example.org/ns/porder"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
  <plt:partnerLinkType name="Write_plt" >
    <plt:role name="Write_role" >
      <plt:portType name="tns:Write_ptt" />
    </plt:role>
  </plt:partnerLinkType>
    <wsdl:types>
      <schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/file/Basics/HelloWorld/WriteData"
              xmlns="http://www.w3.org/2001/XMLSchema" >
        <import namespace="http://www.example.org/ns/porder" schemaLocation="../Schemas/po.xsd" />
      </schema>
    </wsdl:types>
    <wsdl:message name="Write_msg">
        <wsdl:part name="body" element="imp1:PurchaseOrder"/>
    </wsdl:message>
    <wsdl:portType name="Write_ptt">
        <wsdl:operation name="Write">
            <wsdl:input message="tns:Write_msg"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
